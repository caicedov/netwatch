{
  "extends": "@netwatch/tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
        {
            "name": "next"
        }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "next.config.js", ".next/types/**/*.ts"],
  "exclude": ["node_modules", ".next", "out", "build"]
}
